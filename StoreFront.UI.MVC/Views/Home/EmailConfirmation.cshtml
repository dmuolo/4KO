@model StoreFront.UI.MVC.Models.ContactViewModel

@{
    ViewBag.Title = "Email Confirmation";
}

<h2>Thank You</h2>

<div>
    <p>
        Thank you for your message, @Model.FirstName @Model.LastName: <br />"@Model.Message" <br />
        I will respond to you at @Model.Email at my earliest convenience. <br />
        - <em>4KO Support</em>
    </p>
</div>
